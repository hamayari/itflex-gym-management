{# src/Resources/views/produit/_form.html.twig #}

{{ form_start(form) }}

<div class="form-group">
    {{ form_label(form.categorie) }}
    {{ form_widget(form.categorie, {'attr': {'class': 'form-control', 'placeholder': 'Enter category'}}) }}
    {{ form_errors(form.categorie) }}
</div>

<div class="form-group">
    <button type="submit" class="btn btn-primary">Submit</button>
</div>

{{ form_end(form) }}
